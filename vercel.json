{
  "buildCommand": "cd frontend-admin && npm run build",
  "outputDirectory": "frontend-admin/dist",
  "installCommand": "cd frontend-admin && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
